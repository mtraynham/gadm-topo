.SECONDARY:

zip/%.zip:
	mkdir -p $(dir $@)
	curl "https://biogeo.ucdavis.edu/data/gadm3.6/shp/gadm36_$*_shp.zip" -o $@.download
	mv $@.download $@

shp/%: zip/%.zip
	mkdir -p $@
	unzip -d $@ $<
	touch $@

topojson/%.json:
	$(MAKE) shp/$(patsubst %/,%,$(dir $*))
	mkdir -p $(dir $@)
	$(TOPOJSON) -o $@ -p -- shp/$(dir $*)$(patsubst %/,%,$(dir $*))_adm$(notdir $*).shp

clean:
	rm -rf topojson/*
